<%- if controller_name != 'sessions' %>
  <div style="margin-top:30px"><%= link_to "既に登録済みの方はこちら", new_session_path(resource_name), class: "text-white" %><br /></div>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <div style="margin-top:30px"><%= link_to "まだ登録がお済みではない方はこちら", new_registration_path(resource_name), class: "text-white" %><br /></div>
<% end %>
